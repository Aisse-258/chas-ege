{
	"modes": {
		"dev": {
				"build-dir": "./build/dev",
				"tasks": [
					{
						"toolid": "git.get-current-tag-last",
						"dest-var": "version-title"
					},
					{
						"toolid": "git.get-current-tag-exact",
						"dest-var": "version-exact"
					},
					{
						"toolid": "fs.mkdirs",
						"files": [
							"html"
						]
					},
					{
						"toolid": "fs.ln-s",
						"files": [
							["js", "js"],
							["zdn", "zdn"],
							["css", "css"],
							["ext", "ext"]
						]
					},
					{
						"toolid": "jinja.render",
						"templates-dir": "html",
						"dest": "html",
						"templates": [
							"index.html",
							"random-task.html",
							"debug-shell.html",
							"auto-api-reference.html",
							"formula-helper.html",
							"task-samples.html",
							"matrix-helper.html",
							"mini.html",
							"print.html",
							"multi-task.html",
							"declension-helper.html",
							"sorter.html",
							"test.html",
							"time-test.html",
							"contrib.html"
						]
					}
				]
		}
	}	
}
